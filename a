-- DeÄŸiÅŸkenler
local isRespawning = false
local isHeaderRunning = false

-- Function to check player health and reset if below 25
function checkHealthAndReset()
    local character = LocalPlayer.Character
    if character and character:FindFirstChildOfClass("Humanoid") and not isRespawning then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid.Health < 25 then
            print("âš ï¸ SaÄŸlÄ±k 25'in altÄ±nda! Karakter resetleniyor...")
            
            -- Ã–nce tÃ¼m iÅŸlemleri durdur
            isRespawning = true
            isHeaderRunning = false
            print("ğŸ›‘ Header fonksiyonu durduruldu, mevcut iÅŸlemler iptal ediliyor...")
            
            -- Karakteri Ã¶ldÃ¼r
            humanoid.Jump = true
            humanoid.Health = 0
            humanoid:BreakJoints()
            
            -- Yeniden doÄŸma olayÄ±nÄ± dinle ve header'Ä± tekrar baÅŸlat
            local connection
            connection = LocalPlayer.CharacterAdded:Connect(function()
                task.wait(3) -- Karakterin tam olarak yÃ¼klenmesi iÃ§in bekle
                print("âœ… Karakter yeniden doÄŸdu, header fonksiyonu tekrar baÅŸlatÄ±lÄ±yor...")
                isRespawning = false
                connection:Disconnect() -- OlayÄ± dinlemeyi bÄ±rak
                task.spawn(header) -- Header'Ä± yeniden baÅŸlat
            end)
            
            return true
        end
    end
    return false
end



