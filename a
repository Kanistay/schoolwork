task.spawn(function()
	while true do
		local currentPing = getPing()
		local pingFactor = math.clamp(1 - (currentPing / 300), 0, 1) -- yÃ¼ksek ping = yavaÅŸlat

		local fpsFactor = math.clamp(fps / 60, 0.5, 1.2) -- dÃ¼ÅŸÃ¼k fps = yavaÅŸlat

