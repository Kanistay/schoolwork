-- Server Hop ve Otomatik BaÅŸlatma Sistemi
local TeleportCheck = false
local KeepAutoLoad = true

-- Oyun yÃ¼klendikten sonra baÅŸlamak iÃ§in kontrol
if not game:IsLoaded() then
    print("â³ Oyun yÃ¼kleniyor, script baÅŸlatÄ±lmadan Ã¶nce bekleniyor...")
    game.Loaded:Wait()
    print("âœ… Oyun yÃ¼klendi, script baÅŸlatÄ±lÄ±yor...")
end


local VirtualUser = game:service('VirtualUser')

game:service('Players').LocalPlayer.Idled:connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Teleport fonksiyonlarÄ± farklÄ± exploit'ler iÃ§in
local queueonteleport = (syn and syn.queue_on_teleport) or queue_on_teleport or (fluxus and fluxus.queue_on_teleport) or queueteleport

if not queueonteleport then
    warn("âš ï¸ queue_on_teleport fonksiyonu bulunamadÄ±! Server hop sonrasÄ± otomatik baÅŸlatma Ã§alÄ±ÅŸmayabilir.")
end

-- Teleport olayÄ±nÄ± dinle
LocalPlayer.OnTeleport:Connect(function(State)
    if State == Enum.TeleportState.Started then
        if KeepAutoLoad and not TeleportCheck and queueonteleport then
            TeleportCheck = true
            print("ğŸ”„ Server hop algÄ±landÄ±! Otomatik yeniden baÅŸlatma ayarlanÄ±yor...")
            -- Mevcut script'i Ã§alÄ±ÅŸtÄ±racak bir kod parÃ§asÄ± server hop sonrasÄ± Ã§alÄ±ÅŸtÄ±rÄ±lacak
            queueonteleport([[
                print("ğŸ”„ Server hop sonrasÄ± otomatik baÅŸlatma Ã§alÄ±ÅŸÄ±yor...")
                task.wait(3) -- Oyunun tamamen yÃ¼klenmesi iÃ§in biraz bekle
                loadstring(game:HttpGet('https://raw.githubusercontent.com/Kanistay/schoolwork/refs/heads/main/b'))()
            ]])
            print("âœ… Otomatik yeniden baÅŸlatma ayarlandÄ±!")
        end
    end
end)

-- Teleport baÅŸarÄ±sÄ±z olursa
game:GetService("TeleportService").TeleportInitFailed:Connect(function(player, teleportResult, errorMessage)
    if player == LocalPlayer then
        warn("âŒ Teleport baÅŸarÄ±sÄ±z oldu: " .. errorMessage)
        TeleportCheck = false -- Teleport baÅŸarÄ±sÄ±z, bayraÄŸÄ± sÄ±fÄ±rla
    end
end)

